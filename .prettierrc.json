{
    "singleQuote": true,
    "jsxSingleQuote": true,
    "semi": true,
    "tabWidth": 4,
    "bracketSpacing": true,
    "arrowParens": "always",
    "trailingComma": "es5",
    "plugins": ["@ianvs/prettier-plugin-sort-imports"],
    "importOrder": [
        "<BUILTIN_MODULES>",
        "<THIRD_PARTY_MODULES>",
        "",
        "^@(.*)$",
        "",
        "^[./]"
    ]
}
